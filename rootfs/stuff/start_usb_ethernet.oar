#!/bin/sh
cd `dirname $0`
modprobe -r pcconn_mon
lsmod | grep arcotg_udc || modprobe arcotg_udc
lsmod | grep g_ether || modprobe g_ether

sleep 3

ifconfig usb0 10.0.0.2 up

